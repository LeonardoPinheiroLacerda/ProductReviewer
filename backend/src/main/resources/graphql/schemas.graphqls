type Category {
    id: ID!
    description: String!
    properties: [Property] 
}

type Product {
    id: ID!
    description: String!
    price: Float
    category: Category
    properties: [ProductProperty]
}

type ProductProperty {
    product: Product!
    property: Property!
    value: String!
}
type Property {
    id: ID!
    name :String!
    defaultValue :String!
    type :Type!
    category: Category
}

enum Type {
    INTEGER
    DOUBLE
    BOOLEAN
    STRING
}