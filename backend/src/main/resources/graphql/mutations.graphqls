type Mutation {
    createCategory(categoryInput: CategoryInput): Category
    updateCategory(id: ID, categoryInput: CategoryInput): Category
    deleteCategory(id: ID): ID

    createProperty(propertyInput: PropertyInput): Property
    updateProperty(id: ID, propertyInput: PropertyInput): Property
    deleteProperty(id: ID): ID

    createProduct(productInput: ProductInput): Product
    updateProduct(id: ID, productInput: ProductInput): Product
    deleteProduct(id: ID): ID
}